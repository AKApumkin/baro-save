<Items>
  <!--	Der Schnitter™ - Unterwasser Scooter	-->
  <Item name="" identifier="oxygear_scooter" category="Equipment" Tags="mediumitem,provocative,oxygear" cargocontaineridentifier="metalcrate" Scale="0.5" sonarsize="20" impactsoundtag="impact_metal_heavy" health="200" DamagedByProjectiles="true" DamagedByMeleeWeapons="true" DamagedByMonsters="true" DamagedByExplosions="true" ExplosionDamageMultiplier="0.5" fireproof="false" isshootable="true" ImpactTolerance="1.0">
    <!-- Verworfen: ImpactTolerance="10.0" -->
    <Sprite texture="oxygear_scooter_v2.png" depth="0.55" sourcerect="0,0,170,85" />
    <!-- Die Dichte bestimmt, wie schnell ein Objekt sinkt	-->
    <Body width="170" height="85" density="10.10" />
    <!-- Akustische und Optische Wahrnehmbarkeit durch Kreaturen / Sonaridentifizierung	-->
    <AiTarget soundrange="4000" maxsightrange="4000" sonarlabel="entityname.oxygear_scooter" />
    <Holdable slots="Any,RightHand+LeftHand" aimpos="90,0" handle1="-25,32" handle2="-27,34" controlpose="false" msg="ItemMsgPickUpSelect">
      <!-- Lichtsteuerung #1 - Trage- und Batterieerkennung	-->
      <StatusEffect type="OnContained" target="This,Character" Voltage="1.0" setvalue="true">
        <Conditional IsDead="false" />
        <RequiredItems items="mobilebattery" type="Contained" mincondition="0.01" />
      </StatusEffect>
      <StatusEffect type="OnNotContained" target="This,Character" Voltage="0.0" setvalue="true" />
    </Holdable>
    <!-- Lichtsteuerung #2 - Wasser-Erkennung - steuert JEDE Lichtquelle an -->
    <ItemComponent characterusable="false">
      <StatusEffect type="InWater" target="This" targetitemcomponent="LightComponent" IsOn="true" delay="0.5" checkconditionalalways="true">
        <RequiredItem item="mobilebattery" type="Contained" mincondition="0.01" />
      </StatusEffect>
      <StatusEffect type="NotInWater" target="This" targetitemcomponent="LightComponent" IsOn="false" delay="2.5" />
    </ItemComponent>
    <!--
						Schadensoutput-Position über "barrelpos" einstellen = Offset von der Objektmitte aus in Pixeln
						Benutzbarkeit einstellen über "usablein" = Air/Water/Both/None
			-->
    <RepairTool range="20" usablein="Both" repairmultiple="true" barrelpos="45,10" structuredamage="10" itemdamage="10" targetimpulse="10" targetforce="400" allowhitmultiple="true" hitscan="true" spread="0" unskilledspread="0" combatpriority="10" fireprobability="0.0" structurefixamount="0.0" repairthroughwalls="false" repairthroughholes="false">
      <RequiredItems items="mobilebattery" type="Contained" mincondition="0.01" />
      <StatusEffect type="OnUse" target="UseTarget" severlimbsprobability="0.5">
        <Affliction identifier="stun" strength="0.5" />
        <Affliction identifier="bleeding" strength="1" />
        <Affliction identifier="lacerations" strength="1" />
      </StatusEffect>
      <!-- Der Schnitter ™ - Trefferfeedback -->
      <StatusEffect type="OnUse" targettype="UseTarget" stackable="true" disabledeltatime="true">
        <Conditional entitytype="eq Character" />
        <ParticleEmitter particle="blood" copyentityangle="true" anglemin="-10" anglemax="10" particleamount="4" velocitymin="-10" velocitymax="-200" scalemin="0.3" scalemax="0.6" />
        <Sound file="%ModDir%/Sounds/oxyscooter_schnittern.ogg" type="UseTarget" selectionmode="All" range="800" loop="true" />
      </StatusEffect>
    </RepairTool>
    <!-- Positionslicht	(ohne Stromverbrauch) -->
    <LightComponent range="45.0" lightcolor="160,40,0,255" IsOn="false" powerconsumption="0" blinkfrequency="0.0" flicker="0.1" flickerspeed="0.8" castshadows="false" allowingameediting="false" vulnerabletoemp="false">
      <LightTexture texture="%ModDir%/Lights/oxygear_scooter_light.png" origin="0.5,0.5" />
      <Sprite texture="%ModDir%/Lights/oxygear_scooter_light.png" sourcerect="0,0,170,85" alpha="1.0" origin="0.5,0.5" />
    </LightComponent>
    <Propulsion force="300" usablein="water" particles="bubbles">
      <RequiredItems items="mobilebattery" type="Contained" msg="ItemMsgBatteryCellRequired" />
      <StatusEffect type="OnUse" target="Contained" Condition="-0.50">
        <RequiredItem items="batterycell" type="Contained" />
      </StatusEffect>
      <StatusEffect type="OnUse" target="Contained" Condition="-0.255">
        <RequiredItem items="fulguriumbatterycell" type="Contained" />
      </StatusEffect>
      <StatusEffect type="OnUse" target="Contained" Condition="-0.50">
        <RequiredItem excludedidentifiers="batterycell,fulguriumbatterycell" type="Contained" />
      </StatusEffect>
      <sound file="Content/Items/Diving/ScooterLoop.ogg" type="OnUse" range="500.0" loop="true" />
      <!-- Warnton bei niedriger Batterieleistung -->
      <StatusEffect type="OnUse" target="Contained">
        <RequiredItem items="mobilebattery" type="Contained" />
        <Conditional condition="lt 10.0" />
        <Sound file="%ModDir%/Sounds/oxyscooter_warning.ogg" range="250" />
      </StatusEffect>
      <!-- Frontscheinwerfer - Kaltweiss (mit Stromverbrauch) -->
      <LightComponent lightcolor="235,245,255,255" range="1000" powerconsumption="0.01" allowingameediting="false">
        <LightTexture texture="Content/Lights/lightcone.png" origin="-0.01, 0.5" size="1.0,1.0" />
      </LightComponent>
    </Propulsion>
    <ItemContainer capacity="1" maxstacksize="1" containedstateindicatorstyle="battery" hideitems="true">
      <Containable items="mobilebattery" />
    </ItemContainer>
    <PreferredContainer primary="divingcab,locker" minamount="1" maxamount="1" spawnprobability="0.33" />
    <!--	Reparatureinstellungen -->
    <Repairable selectkey="Action" header="mechanicalrepairsheader" Msg="ItemMsgRepairWrench" hudpriority="5" deteriorationspeed="0" mindeteriorationdelay="0" maxdeteriorationdelay="0" RepairThreshold="90" fixDurationHighSkill="5" fixDurationLowSkill="10">
      <GuiFrame relativesize="0.2,0.16" minsize="400,200" maxsize="480,216" anchor="Center" relativeoffset="0.0,0.27" style="ItemUI" />
      <RequiredSkill identifier="mechanical" level="42" />
      <RequiredItem items="wrench" type="Equipped" />
      <ParticleEmitter particle="damagebubbles" particleburstamount="2" particleburstinterval="2.0" particlespersecond="2" scalemin="0.5" scalemax="1.5" anglemin="0" anglemax="359" velocitymin="-10" velocitymax="10" mincondition="0.0" maxcondition="50.0" />
      <ParticleEmitter particle="smoke" particleburstamount="3" particleburstinterval="0.5" particlespersecond="1" scalemin="2" scalemax="2.5" anglemin="0" anglemax="359" velocitymin="-50" velocitymax="50" mincondition="15.0" maxcondition="50.0" />
      <ParticleEmitter particle="heavysmoke" particleburstinterval="0.25" particlespersecond="1" scalemin="2.5" scalemax="5.0" mincondition="0.0" maxcondition="15.0" />
      <StatusEffect type="OnFailure" target="Character" targetlimbs="LeftHand,RightHand">
        <Sound file="Content/Items/MechanicalRepairFail.ogg" range="500" />
        <Affliction identifier="lacerations" strength="1" />
      </StatusEffect>
      <!-- Der Schnitter™ explodiert wenn er zerstört wird (Kondition = 0) -->
      <StatusEffect type="OnBroken" targettype="This" checkconditionalalways="true" delay="2.0">
        <sound file="Content/Items/Weapons/ExplosionSmall3.ogg" range="5000" selectionmode="All" />
        <sound file="Content/Items/Weapons/ExplosionDebris3.ogg" range="5000" />
        <sound file="Content/Items/Weapons/StunGrenade.ogg" range="5000" />
        <Explosion range="500.0" structuredamage="100" itemdamage="50" force="400.0" severlimbsprobability="1.0" decal="explosion" decalsize="0.5">
          <Affliction identifier="burn" strength="25" />
          <Affliction identifier="explosiondamage" strength="100" />
          <Affliction identifier="stun" strength="15" />
        </Explosion>
        <Remove />
      </StatusEffect>
    </Repairable>
    <Fabricate suitablefabricators="fabricator" requiredtime="30">
      <RequiredSkill identifier="mechanical" level="40" />
      <RequiredItem identifier="fpgacircuit" />
      <RequiredItem identifier="plastic" />
      <RequiredItem identifier="aluminium" />
    </Fabricate>
    <Deconstruct time="15">
      <Item identifier="fpgacircuit" />
      <Item identifier="plastic" />
      <Item identifier="aluminium" />
    </Deconstruct>
    <Price baseprice="250">
      <Price locationtype="outpost" multiplier="1.25" minavailable="2" />
      <Price locationtype="city" multiplier="1.00" minavailable="4" />
      <Price locationtype="research" multiplier="1.50" minavailable="2" />
      <Price locationtype="military" multiplier="0.75" minavailable="10" />
      <Price locationtype="mine" multiplier="1.25" minavailable="2" />
    </Price>
  </Item>
